<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.billSplitter"
      android:versionCode="1" android:installLocation="auto">
    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".billSplitter" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    <activity android:name=".results" android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
<!--                <category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
    </activity>
</application>


<uses-sdk android:targetSdkVersion="8" android:maxSdkVersion="8" android:minSdkVersion="8"></uses-sdk>
</manifest> 